{
  "name": "data-marketplace-indexer",
  "version": "1.0.0",
  "description": "Search indexer service for Data marketplace",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "func start",
    "test": "jest",
    "lint": "eslint \"**/*.{j,t}s\"",
    "format": "prettier --write \"**/*.{j,t}s\"",
    "prepare": "husky install",
    "azurite": "azurite",
    "dev": "npm-run-all --parallel watch start"
  },
  "lint-staged": {
    "*.{j,t}s": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@algolia/client-search": "^4.14.3",
    "@azure/functions": "^3.5.1",
    "@equinor/data-marketplace-models": "^3.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.x",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "azure-functions-core-tools": "^4.0.5095",
    "azurite": "^3.22.0",
    "eslint": "^8.34.0",
    "eslint-plugin-import": "^2.27.5",
    "gitmoji-cli": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@portabletext/toolkit": "^2.0.1",
    "algoliasearch": "^4.14.3",
    "axios": "^1.3.5",
    "dotenv-azure": "^2.0.0",
    "fp-ts": "^2.13.1"
  }
}
